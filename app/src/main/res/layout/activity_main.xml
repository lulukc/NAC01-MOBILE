<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/mercadoLivre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="42dp"
        android:onClick="setWebView"
        android:text="Mercado livre"
        app:layout_constraintBaseline_toBaselineOf="@+id/buscape"
        app:layout_constraintEnd_toStartOf="@+id/buscape" />

    <Button
        android:id="@+id/buscape"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="81dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="82dp"
        android:onClick="setWebView"
        android:text="BuscapÃ©"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ebay"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ebay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="22dp"
        android:onClick="setWebView"
        android:text="eBay"
        app:layout_constraintEnd_toStartOf="@+id/Webmotors"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mercadoLivre" />

    <Button
        android:id="@+id/Webmotors"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:onClick="setWebView"
        android:text="Webmotors"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ebay"
        app:layout_constraintTop_toBottomOf="@+id/buscape" />

    <Button
        android:id="@+id/magalu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:onClick="setWebView"
        android:text="Magalu"
        app:layout_constraintStart_toStartOf="@+id/ebay"
        app:layout_constraintTop_toBottomOf="@+id/ebay" />

    <Button
        android:id="@+id/netshoes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="34dp"
        android:onClick="setWebView"
        android:text="Netshoes"
        app:layout_constraintStart_toStartOf="@+id/Webmotors"
        app:layout_constraintTop_toBottomOf="@+id/Webmotors" />

    <Button
        android:id="@+id/americanas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="43dp"
        android:onClick="setWebView"
        android:text="Americanas"
        app:layout_constraintEnd_toStartOf="@+id/submarino"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/magalu" />

    <Button
        android:id="@+id/submarino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:onClick="setWebView"
        android:text="Submarino"
        app:layout_constraintBaseline_toBaselineOf="@+id/americanas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/americanas" />

    <Button
        android:id="@+id/Sobre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="97dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="110dp"
        android:onClick="sobreView"
        android:text="Sobre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/submarino"
        app:layout_constraintStart_toStartOf="@+id/mercadoLivre" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Sobre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="110dp"
        android:onClick="sobreView"
        android:text="Sobre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/webmotors" />

    <ImageView
        android:id="@+id/mercadoLivre"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="39dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toTopOf="@+id/netshoes"
        app:layout_constraintEnd_toStartOf="@+id/magalu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mercado_livre" />

    <ImageView
        android:id="@+id/magalu"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="47dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toTopOf="@+id/ebay"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mercadoLivre"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/magalu" />

    <ImageView
        android:id="@+id/netshoes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="25dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toTopOf="@+id/buscape"
        app:layout_constraintEnd_toStartOf="@+id/ebay"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mercadoLivre"
        app:srcCompat="@drawable/netshoes" />

    <ImageView
        android:id="@+id/ebay"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toTopOf="@+id/webmotors"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/netshoes"
        app:layout_constraintTop_toBottomOf="@+id/magalu"
        app:srcCompat="@drawable/ebay" />

    <ImageView
        android:id="@+id/buscape"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="245dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/webmotors"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/netshoes"
        app:srcCompat="@drawable/buscape" />

    <ImageView
        android:id="@+id/webmotors"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="87dp"
        android:onClick="setWebView"
        app:layout_constraintBottom_toTopOf="@+id/Sobre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buscape"
        app:layout_constraintTop_toBottomOf="@+id/ebay"
        app:srcCompat="@drawable/webmotors" />
</androidx.constraintlayout.widget.ConstraintLayout>